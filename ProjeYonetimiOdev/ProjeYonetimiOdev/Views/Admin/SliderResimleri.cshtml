
@{
    ViewBag.Title = "SliderResimleri";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class="card card-dark" style="margin:5px;">
    <div class="card-header bg-cyan">
        <h3 class="card-title">Slider Resim Ekle</h3>
    </div>
    <form class="form-horizontal" method="post" action="/Admin/SliderResimEkle" enctype="multipart/form-data">
        <div class="card-body">


            <div class="form-group row">
                <label for="" class="col-sm-2 col-form-label">Slider Resmi : </label>
                <div class="col-sm-10">
                    <div class="input-group">
                        <div class="custom-file">
                            <input type="file" id="" name="fileUpload"><br><br>
                        </div>

                    </div>
                </div>
            </div>

        </div>
        <!-- /.card-body -->
        <div class="card-footer">
            <button type="submit" class="btn bg-cyan">Kaydet</button>
        </div>
        <!-- /.card-footer -->
    </form>
</div>

